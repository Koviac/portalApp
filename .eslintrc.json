{
  "env": {
      "browser": true,
      "es6": true,
      "node": true
  },
  "extends": [
    "airbnb-base",
    "plugin:angular/johnpapa",
    "plugin:node/recommended",
    "plugin:security/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "linebreak-style": 0,
    "func-names": "off",
    "node/no-unpublished-require": 0,
    "no-console": "off"
  }
}
